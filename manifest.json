{
  "manifest_version": 2,
  "name": "ZoomPump",
  "version": "1.0.0",
  "browser_action": {
    "default_title": "ZoomPump",
    "default_popup": "./popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "script.js"]
    }
  ],
  "options_page": "./options/options.html"
}
